{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,

    "resolveJsonModule": true

    "resolveJsonModule": true,
    "types": ["node"]

  },
  "include": ["src", "prisma/seed.ts"],
  "exclude": ["node_modules", "dist"]
}
